{
  "name": "ng-particles",
  "version": "3.0.0-beta.4",
  "description": "Official tsParticles Angular Component - Easily create highly customizable particle animations and use them as animated backgrounds for your website. Ready to use components available also for Web Components, React, Vue.js (2.x and 3.x), Svelte, jQuery, Preact, Riot.js, Solid.js, Inferno.",
  "keywords": [
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react",
    "reactjs",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:all": "ng build",
    "build": "node scripts/prebuild.js && ng build ng-particles --configuration production",
    "version": "yarn build && git add ./projects/ng-particles/package.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepack": "yarn build"
  },
  "publishConfig": {
    "directory": "./dist/ng-particles"
  },
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.1",
    "tsparticles-engine": "^2.0.0-beta.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
