{
  "name": "react-particles",
  "version": "2.3.1",
  "description": "Official tsParticles React Component - Easily create highly customizable particle, confetti and fireworks animations and use them as animated backgrounds for your website. Ready to use components available also for Web Components, Vue.js (2.x and 3.x), Angular, Svelte, jQuery, Preact, Riot.js, Solid.js, Inferno.",
  "scripts": {
    "clear:dist": "rimraf -f ./dist",
    "prettify:ci:src": "prettier --check ./src",
    "prettify:ci:readme": "prettier --check ./README.md",
    "prettify:src": "prettier --write ./src",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx",
    "build": "pnpm run clear:dist && pnpm run prettify:src && pnpm run build:ts:check && pnpm run lint && pnpm run build:ts && webpack --mode production && pnpm run prettify:readme && pnpm run distfiles",
    "build:ci": "pnpm run clear:dist && pnpm run prettify:ci:src && pnpm run build:ts:check && pnpm run lint:ci && pnpm run build:ts && webpack --mode production && pnpm run prettify:ci:readme && pnpm run distfiles",
    "build:ts:check": "tsc --noEmit",
    "build:ts": "pnpm run build:ts:types && pnpm run build:ts:browser && pnpm run build:ts:cjs && pnpm run build:ts:esm && pnpm run build:ts:umd",
    "build:ts:browser": "tsc -b tsconfig.browser.json",
    "build:ts:cjs": "tsc",
    "build:ts:esm": "tsc -b tsconfig.module.json",
    "build:ts:types": "tsc -b tsconfig.types.json",
    "build:ts:umd": "tsc -b tsconfig.umd.json",
    "distfiles": "node ./scripts/distfiles.js",
    "version": "pnpm run distfiles  && git add package.dist.json",
    "prepack": "pnpm run build",
    "install": "node ./scripts/install.js"
  },
  "repository": {
    "url": "https://github.com/matteobruni/tsparticles",
    "directory": "components/react",
    "type": "git"
  },
  "keywords": [
    "front-end",
    "frontend",
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react-particles.js",
    "react-particles",
    "react",
    "reactjs",
    "vue-particles",
    "ngx-particles",
    "angular-particles",
    "particleground",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "confetti",
    "canvas",
    "fireworks",
    "fireworks-js",
    "confetti-js",
    "confettijs",
    "fireworksjs",
    "canvas-confetti"
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/matteobruni"
    },
    {
      "type": "buymeacoffee",
      "url": "https://www.buymeacoffee.com/matteobruni"
    }
  ],
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "author": "Matteo Bruni <matteo.bruni@me.com>",
  "license": "MIT",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "react": "^18.2.0",
    "tsparticles-engine": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/runtime": "^7.19.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
