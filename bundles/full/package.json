{
  "name": "tsparticles",
  "version": "2.1.3",
  "description": "Easily create highly customizable particle animations and use them as animated backgrounds for your website. Ready to use components available also for React, Vue.js (2.x and 3.x), Angular, Svelte, jQuery, Preact, Riot.js, Inferno.",
  "homepage": "https://particles.js.org/",
  "scripts": {
    "prettify:ci:src": "prettier --check ./src",
    "prettify:ci:readme": "prettier --check ./README.md",
    "prettify:src": "prettier --write ./src",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx",
    "compile": "yarn build:ts",
    "build:ts": "yarn build:ts:types && yarn build:ts:browser && yarn build:ts:cjs && yarn build:ts:esm && yarn build:ts:umd",
    "build:ts:browser": "tsc -b tsconfig.browser.json",
    "build:ts:cjs": "tsc",
    "build:ts:esm": "tsc -b tsconfig.module.json",
    "build:ts:types": "tsc -b tsconfig.types.json",
    "build:ts:umd": "tsc -b tsconfig.umd.json",
    "build:docs": "rimraf -f docs && typedoc",
    "bundle": "webpack --mode production",
    "distfiles": "node ./scripts/distfiles.js",
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && yarn compile && yarn bundle && yarn prettify:readme && yarn distfiles && yarn build:docs",
    "build:ci": "yarn clear:dist && yarn prettify:ci:src && yarn lint:ci && yarn compile && yarn bundle && yarn prettify:ci:readme && yarn distfiles && yarn build:docs",
    "clear:dist": "rimraf -f ./dist",
    "version": "yarn distfiles && git add package.dist.json",
    "prepack": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "bundles/full"
  },
  "keywords": [
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react-particles.js",
    "react-particles",
    "react",
    "reactjs",
    "vue-particles",
    "ngx-particles",
    "angular-particles",
    "particleground",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "confetti",
    "canvas",
    "fireworks",
    "fireworks-js",
    "confetti-js",
    "confettijs",
    "fireworksjs",
    "canvas-confetti"
  ],
  "author": "Matteo Bruni <matteo.bruni@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/matteobruni"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typedocMain": "./src/index.ts",
  "prettier": "@tsparticles/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@tsparticles/eslint-config": "^1.0.2",
    "@tsparticles/prettier-config": "^1.0.0",
    "@tsparticles/tsconfig": "^1.0.0",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.20.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-tsparticles-plugin": "^1.1.1"
  },
  "dependencies": {
    "tsparticles-engine": "^2.1.3",
    "tsparticles-interaction-external-trail": "^2.1.3",
    "tsparticles-plugin-absorbers": "^2.1.3",
    "tsparticles-plugin-emitters": "^2.1.3",
    "tsparticles-plugin-polygon-mask": "^2.1.3",
    "tsparticles-slim": "^2.1.3",
    "tsparticles-updater-roll": "^2.1.3",
    "tsparticles-updater-tilt": "^2.1.3",
    "tsparticles-updater-twinkle": "^2.1.3",
    "tsparticles-updater-wobble": "^2.1.3"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
