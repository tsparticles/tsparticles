{
  "name": "tsparticles-particles.js",
  "version": "2.3.0",
  "description": "Easily create highly customizable particle animations and use them as animated backgrounds for your website. Ready to use components available also for React, Vue.js (2.x and 3.x), Angular, Svelte, jQuery, Preact, Riot.js, Inferno.",
  "homepage": "https://particles.js.org",
  "scripts": {
    "build": "tsparticles-build",
    "build:ci": "tsparticles-build --ci",
    "build:docs": "rimraf -f docs && npx typedoc",
    "version": "tsparticles-build -d && git add package.dist.json",
    "prepack": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "bundles/pjs"
  },
  "keywords": [
    "front-end",
    "frontend",
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react-particles.js",
    "react-particles",
    "react",
    "reactjs",
    "vue-particles",
    "ngx-particles",
    "angular-particles",
    "particleground",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "confetti",
    "canvas",
    "fireworks",
    "fireworks-js",
    "confetti-js",
    "confettijs",
    "fireworksjs",
    "canvas-confetti"
  ],
  "author": "Matteo Bruni <matteo.bruni@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/matteobruni"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "tsParticles Particles.js integration",
    "readmeFile": "./README.md"
  },
  "prettier": "@tsparticles/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@tsparticles/build": "workspace:^1.2.0",
    "@tsparticles/eslint-config": "workspace:^1.2.0",
    "@tsparticles/prettier-config": "workspace:^1.2.0",
    "@tsparticles/tsconfig": "workspace:^1.2.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.21.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-tsparticles-plugin": "workspace:^1.3.0"
  },
  "dependencies": {
    "tsparticles-engine": "workspace:^2.3.0"
  },
  "publishConfig": {
    "directory": "dist",
