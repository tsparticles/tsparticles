{
  "name": "tsparticles-slim",
  "version": "2.1.1",
  "description": "Easily create highly customizable particle animations and use them as animated backgrounds for your website. Ready to use components available also for React, Vue.js (2.x and 3.x), Angular, Svelte, jQuery, Preact, Riot.js, Inferno.",
  "homepage": "https://particles.js.org/",
  "scripts": {
    "prettify:src": "prettier --write ./src",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "compile": "yarn build:ts",
    "build:ts": "yarn build:ts:types && yarn build:ts:browser && yarn build:ts:cjs && yarn build:ts:esm && yarn build:ts:umd",
    "build:ts:browser": "tsc -b tsconfig.browser.json",
    "build:ts:cjs": "tsc",
    "build:ts:esm": "tsc -b tsconfig.module.json",
    "build:ts:types": "tsc -b tsconfig.types.json",
    "build:ts:umd": "tsc -b tsconfig.umd.json",
    "bundle": "webpack --mode production",
    "distfiles": "node ./scripts/distfiles.js",
    "build": "yarn clear:dist && yarn prettify:src && yarn compile && yarn lint && yarn bundle && yarn prettify:readme && yarn distfiles",
    "clear:dist": "rimraf -f ./dist",
    "version": "yarn distfiles && git add package.dist.json",
    "prepack": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "bundles/slim"
  },
  "keywords": [
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react-particles.js",
    "react-particles",
    "react",
    "reactjs",
    "vue-particles",
    "ngx-particles",
    "angular-particles",
    "particleground",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "confetti",
    "canvas",
    "fireworks",
    "fireworks-js",
    "confetti-js",
    "confettijs",
    "fireworksjs",
    "canvas-confetti"
  ],
  "author": "Matteo Bruni <matteo.bruni@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/matteobruni"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@tsparticles/eslint-config": "^1.0.1",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.20.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.7.3",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-tsparticles-plugin": "^1.1.1"
  },
  "dependencies": {
    "tsparticles-engine": "^2.1.1",
    "tsparticles-interaction-external-attract": "^2.1.1",
    "tsparticles-interaction-external-bounce": "^2.1.1",
    "tsparticles-interaction-external-bubble": "^2.1.1",
    "tsparticles-interaction-external-connect": "^2.1.1",
    "tsparticles-interaction-external-grab": "^2.1.1",
    "tsparticles-interaction-external-pause": "^2.1.1",
    "tsparticles-interaction-external-push": "^2.1.1",
    "tsparticles-interaction-external-remove": "^2.1.1",
    "tsparticles-interaction-external-repulse": "^2.1.1",
    "tsparticles-interaction-particles-attract": "^2.1.1",
    "tsparticles-interaction-particles-collisions": "^2.1.1",
    "tsparticles-interaction-particles-links": "^2.1.1",
    "tsparticles-move-base": "^2.1.1",
    "tsparticles-move-parallax": "^2.1.1",
    "tsparticles-particles.js": "^2.1.1",
    "tsparticles-shape-circle": "^2.1.1",
    "tsparticles-shape-image": "^2.1.1",
    "tsparticles-shape-line": "^2.1.1",
    "tsparticles-shape-polygon": "^2.1.1",
    "tsparticles-shape-square": "^2.1.1",
    "tsparticles-shape-star": "^2.1.1",
    "tsparticles-shape-text": "^2.1.1",
    "tsparticles-updater-angle": "^2.1.1",
    "tsparticles-updater-color": "^2.1.1",
    "tsparticles-updater-life": "^2.1.1",
    "tsparticles-updater-opacity": "^2.1.1",
    "tsparticles-updater-out-modes": "^2.1.1",
    "tsparticles-updater-size": "^2.1.1",
