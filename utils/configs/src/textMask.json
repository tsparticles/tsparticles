{
    "name": "Text Mask",
    "smooth": true,
    "interactivity": {
        "events": {
            "onHover": {
                "enable": true,
                "mode": "bubble",
                "parallax": {
                    "enable": false,
                    "force": 2,
                    "smooth": 10
                }
            }
        },
        "modes": {
            "bubble": {
                "distance": 40,
                "duration": 2,
                "opacity": 8,
                "size": 15
            }
        }
    },
    "particles": {
        "move": {
            "direction": "none",
            "distance": 2,
            "enable": true,
            "speed": 1
        },
        "number": {
            "value": 600
        },
        "color": {
            "value": "random"
        },
        "shape": {
            "type": ["circle", "square", "triangle"]
        },
        "size": {
            "value": {
                "min": 1,
                "max": 3
            }
        }
    },
    "canvasMask": {
        "enable": true,
        "override": {
            "color": false
        },
        "scale": 1,
        "pixels": {
            "filter": "pixelTextFilter"
        },
        "position": {
            "x": 70,
            "y": 30
        },
        "text": {
            "color": "#ff0000",
            "font": {
                "size": 500
            },
            "text": "Hello"
        }
    },
    "background": {
        "color": "#000000",
        "image": "",
        "position": "50% 50%",
        "repeat": "no-repeat",
        "size": "cover"
    }
}
