{
  "name": "webpack-tsparticles-plugin",
  "version": "1.3.3",
  "main": "dist/webpack-tsparticles.js",
  "types": "dist/webpack-tsparticles.d.ts",
  "license": "MIT",
  "scripts": {
    "prettify:ci:src": "prettier --check ./src/*",
    "prettify:ci:readme": "prettier --check ./README.md",
    "prettify:src": "prettier --write ./src/*",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx",
    "compile": "pnpm run build:ts",
    "compile:ci": "pnpm run build:ts",
    "build:ts": "pnpm run build:ts:cjs",
    "build:ts:cjs": "tsc",
    "build": "pnpm run clear:dist && pnpm run prettify:src && pnpm run lint && pnpm run compile && pnpm run prettify:readme",
    "build:ci": "pnpm run clear:dist && pnpm run prettify:ci:src && pnpm run lint:ci && pnpm run compile && pnpm run prettify:ci:readme",
    "clear:dist": "rimraf -f ./dist",
    "prepack": "pnpm run build"
  },
  "prettier": "@tsparticles/prettier-config",
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@tsparticles/eslint-config": "^1.2.3",
    "@tsparticles/prettier-config": "^1.2.3",
    "@types/node": "^18.8.3",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.21.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  }
}
