{
  "name": "tsparticles-editor",
  "version": "1.0.0-alpha.36",
  "description": "tsParticles Configuration Editor",
  "keywords": [
    "tsparticles",
    "particles.js",
    "particles",
    "editor",
    "dat.gui",
    "javascript",
    "wyiswyg"
  ],
  "author": "Matteo Bruni <ar3s@icloud.com>",
  "homepage": "https://particles.matteobruni.it/",
  "license": "ISC",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/js/tsparticles.editor.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "core/editor"
  },
  "scripts": {
    "prettify:src": "prettier --write ./src/",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src/ts --ext .js,.jsx,.ts,.tsx --fix",
    "clear:dist": "rimraf -f ./dist",
    "build:ts:es": "tsc -b tsconfig.browser.json",
    "build:ts": "tsc",
    "build": "yarn clear:dist && yarn prettify:src && yarn build:ts:es && yarn lint && webpack -p && yarn build:ts && yarn prettify:readme",
    "prepare": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "dependencies": {
    "object-gui": "^1.0.0-alpha.12",
    "tslib": "^2.0.0",
    "tsparticles": "^1.18.0-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "fibers": "^5.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "terser-webpack-plugin": "^3.0.7",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
