{
  "private": true,
  "name": "tsparticles-workspace",
  "description": "tsParticles monorepository",
  "version": "2.9.3",
  "scripts": {
    "build": "pnpm run prettify:readme && lerna run build",
    "build:ci": "pnpm run prettify:ci:readme && lerna run build:ci",
    "build:docs": "rimraf docs && typedoc",
    "prettify:ci:readme": "prettier --check ./README.md && prettier --check ./markdown/*",
    "prettify:readme": "prettier --write ./README.md && prettier --write ./markdown/*",
    "version:alpha": "lerna version prerelease --preid alpha --conventional-commits",
    "version:beta": "lerna version prerelease --preid beta --conventional-commits",
    "publish:alpha": "lerna publish from-package --pre-dist-tag alpha --preid alpha",
    "publish:beta": "lerna publish from-package --pre-dist-tag beta --preid beta",
    "publish:next": "lerna publish from-package --dist-tag next",
    "publish:v1": "lerna publish from-package --dist-tag v1",
    "deploy:site": "lerna run deploy --scope website",
    "prepare": "husky install"
  },
  "prettier": "@tsparticles/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@nrwl/devkit": "^15.9.4",
    "@nrwl/nx-cloud": "^15.3.5",
    "@nrwl/workspace": "^15.9.4",
    "@skypack/package-check": "^0.2.2",
    "@tsparticles/cli": "^1.4.1",
    "@tsparticles/eslint-config": "^1.12.0",
    "@tsparticles/prettier-config": "^1.10.0",
    "@tsparticles/tsconfig": "^1.13.0",
    "@tsparticles/webpack-plugin": "^1.14.0",
    "@types/chai": "^4.3.5",
    "@types/jsdom": "^21.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-loader": "^9.1.2",
    "browserslist": "^4.21.7",
    "canvas": "^2.11.2",
    "chai": "^4.3.7",
    "compare-versions": "^6.0.0-rc.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "ini": "^4.1.1",
    "install": "^0.13.0",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^6.6.2",
    "mocha": "^10.2.0",
    "nx": "^15.9.4",
    "nx-cloud": "^15.3.5",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.9",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typedoc-plugin-carbon-ads": "^1.4.0",
    "typedoc-plugin-clarity": "^1.4.0",
    "typedoc-plugin-coverage": "^2.1.0",
    "typedoc-plugin-keywords": "^1.4.1",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.1.3",
    "typescript-json-schema": "^0.56.0",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1",
    "yorkie": "^2.0.0"
  }
}

