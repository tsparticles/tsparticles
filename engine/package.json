{
  "name": "tsparticles-engine",
  "version": "2.2.3",
  "description": "Easily create highly customizable particle, confetti and fireworks animations and use them as animated backgrounds for your website. Ready to use components available also for React, Vue.js (2.x and 3.x), Angular, Svelte, jQuery, Preact, Riot.js, Inferno.",
  "homepage": "https://particles.js.org",
  "scripts": {
    "prettify:ci:tests": "prettier --check ./tests",
    "prettify:ci:schema": "prettier --check ./schema/options.schema.json",
    "prettify:tests": "prettier --write ./tests",
    "prettify:schema": "prettier --write ./schema/options.schema.json",
    "lint:ci:tests": "echo eslint --no-eslintrc --config .eslintrc.test.js --ext .js,.jsx,.ts,.tsx tests",
    "lint:tests": "echo eslint --no-eslintrc --config .eslintrc.test.js --ext .js,.jsx,.ts,.tsx --fix tests",
    "slimbuild": "tsparticles-build src",
    "slimbuild:ci": "tsparticles-build src --ci",
    "build": "yarn slimbuild && yarn build:schema && yarn lint:tests && yarn prettify:tests && yarn test && yarn build:docs",
    "build:ci": "yarn slimbuild:ci && yarn build:schema && yarn lint:ci:tests && yarn prettify:ci:tests && yarn test",
    "build:docs": "rimraf -f docs && npx typedoc",
    "build-docs": "yarn build:docs",
    "build:schema": "echo ts-json-schema-generator --path 'src/**/*.ts' --type 'IOptions' -f tsconfig.schema.json --additional-properties true --out ./schema/options.schema.json",
    "test": "nyc mocha",
    "preversion": "yarn test",
    "version": "node ./scripts/postversion.js && tsparticles-build src --distfiles && git add tsParticles.nuspec && git add package.dist.json",
    "postversion": "git push && git push --tags",
    "prepack": "yarn build",
    "release:alpha": "yarn version --prerelease --preid alpha",
    "release:beta": "yarn version --prerelease --preid beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "engine"
  },
  "keywords": [
    "front-end",
    "frontend",
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react-particles.js",
    "react-particles",
    "react",
    "reactjs",
    "vue-particles",
    "ngx-particles",
    "angular-particles",
    "particleground",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "confetti",
    "canvas",
    "fireworks",
    "fireworks-js",
    "confetti-js",
    "confettijs",
    "fireworksjs",
    "canvas-confetti"
  ],
  "author": "Matteo Bruni <matteo.bruni@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/matteobruni"
    },
    {
      "type": "buymeacoffee",
      "url": "https://www.buymeacoffee.com/matteobruni"
    }
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "displayName": "tsParticles Engine",
    "readmeFile": "./README.md"
  },
  "prettier": "@tsparticles/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@tsparticles/build": "^1.1.3",
    "@tsparticles/eslint-config": "^1.1.3",
    "@tsparticles/prettier-config": "^1.1.2",
    "@tsparticles/tsconfig": "^1.1.2",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.1",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.21.3",
    "canvas": "^2.9.3",
    "chai": "^4.3.6",
    "compare-versions": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "install": "^0.13.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.3",
    "ts-json-schema-generator": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.54.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-tsparticles-plugin": "^1.2.3",
    "window": "^4.2.7"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
